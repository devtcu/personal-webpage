{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:fast": "NODE_OPTIONS='--max_old_space_size=4096' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && cp public/_headers out/ && cp -r public/files out/",
    "deploy": "touch out/.nojekyll && git add out/ && git commit -m \"Deploy to GitHub Pages\" && git subtree push --prefix out origin gh-pages",
    "deploy:cache-bust": "./deploy-with-cache-bust.sh"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "autoprefixer": "^10.4.14",
    "file-loader": "^6.2.0",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typewriter-effect": "^2.20.1",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^18",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "13.4.19",
    "postcss": "^8.4.27",
    "svg-to-ico": "^1.0.17",
    "tailwindcss": "^3.3.3",
    "typescript": "^5"
  }
}
